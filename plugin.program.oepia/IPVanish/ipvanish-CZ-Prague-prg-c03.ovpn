client
dev tun
proto udp
remote prg-c03.ipvanish.com 443
resolv-retry infinite
nobind
persist-key
persist-tun
persist-remote-ip
ca /storage/.config/vpn-config/ca.crt
tls-remote prg-c03.ipvanish.com
auth-user-pass /storage/.config/vpn-config/pass.txt
comp-lzo
verb 3
auth SHA256
cipher AES-256-CBC
keysize 256
tls-cipher DHE-RSA-AES256-SHA:DHE-DSS-AES256-SHA:AES256-SHA
